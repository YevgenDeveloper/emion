{
  "name": "endymion",
  "version": "0.1.0",
  "description": "A package to run all odyss√©e back alltogether",
  "main": "index.js",
  "scripts": {
    "test": "nyc ava ",
    "test:watch": "nyc ava --watch",
    "dev": "node --require ts-node/register --require tsconfig-paths/register src/index.ts",
    "dev:debug": "node --require ts-node/register --require tsconfig-paths/register --inspect-brk=9229 src/index.ts",
    "prod": "cross-env NODE_ENV=production node --require ts-node/register --require tsconfig-paths/register  --no-warnings src/index.ts",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "prettier": {
    "semi": false
  },
  "author": "Arthur CARLI",
  "license": "MIT",
  "dependencies": {
    "@types/fs-extra": "8.0.1",
    "@types/jest": "24.0.22",
    "@types/node": "12.12.7",
    "@types/portscanner": "2.1.0",
    "@types/vorpal": "1.12.0",
    "cfonts": "2.4.5",
    "dotenv": "^8.1.0",
    "fs-extra": "8.1.0",
    "portscanner": "2.2.0",
    "simple-git": "1.126.0",
    "vorpal": "1.12.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "0.1.3",
    "@types/dotenv": "^8.2.0",
    "@types/sinon": "7.5.0",
    "ava": "2.4.0",
    "cross-env": "^6.0.0",
    "npm-check-updates": "^3.1.20",
    "nyc": "14.1.1",
    "prettier": "1.19.1",
    "sinon": "7.5.0",
    "source-map-support": "0.5.16",
    "stub-spawn-once": "^2.3.0",
    "tap-notify": "1.0.0",
    "ts-node": "^8.5.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "tslint-config-standard": "^9.0.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "3.7.2",
    "vuepress": "^1.2.0",
    "vuepress-theme-cool": "^1.2.1",
    "vuepress-theme-ououe": "^1.3.8",
    "vuepress-theme-yuu": "^2.2.0"
  },
  "bin": {
    "endymion": "./bin/run.sh"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "exclude": [
      "src/childProcessEventsHandlers.ts",
      "*/*.test.ts",
      "src/pstree.ts"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "ava": {
    "files": [
      "src/**/*.test.ts"
    ],
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register",
      "tsconfig-paths/register",
      "source-map-support/register"
    ]
  }
}
