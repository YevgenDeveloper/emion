{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules"
  ],
  "jsRules": {},
  "rules": {
    "cyclomatic-complexity": [
      true,
      12
    ],
    "trailing-comma": [
      false,
      "always"
    ],
    "semicolon": [
      true,
      "never"
    ],
    "quotemark": [
      true,
      "single",
      "avoid-escape",
      "avoid-template"
    ],
    "ter-indent": [
      true,
      2,
      {
        "MemberExpression": 1,
        "SwitchCase": 1,
        "outerIIFEBody": 1
      }
    ],
    "object-literal-sort-keys": false,
    "space-within-parens": [
      true,
      "never"
    ],
    "space-before-function-paren": false
  },
  "rulesDirectory": []
}
